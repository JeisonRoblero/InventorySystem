{
    "require": {
        "google/apiclient": "2.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "extra": {
        "archive": {
            "exclude": [
                "/tests",
                "/doc",
                "/examples",
                "/.github",
                "/phpunit.xml"
            ]
        }
    }
}
